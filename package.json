    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.0",
    "eslint": "^5.15.1",
    "eslint-config-ali": "^5.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
