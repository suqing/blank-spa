    "eslint-fix": "./node_modules/.bin/eslint --fix --ext .jsx,.js,.tsx,.ts ./src",
    "eslint-output": "node_modules/.bin/eslint -f html --ext .jsx,.js,.tsx,.ts src/ > eslint-result.html"
  },
  "dependencies": {
    "react-loadable": "^5.5.0"
  },
